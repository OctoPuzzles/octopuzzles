{
  "name": "octopuzzles",
  "version": "1.0.0",
  "description": "octopuzzles",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run migrate:up && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .eslintignore --check --plugin-search-dir=. . && eslint --ignore-path .eslintignore .",
    "format": "prettier --ignore-path .eslintignore --write --plugin-search-dir=. .",
    "format:check": "prettier --ignore-path .eslintignore --check --plugin-search-dir=. .",
    "migrate:create": "migrate-mongo create -f migrations/migrate-mongo-config.js",
    "migrate:up": "migrate-mongo up -f migrations/migrate-mongo-config.js",
    "migrate:down": "migrate-mongo down -f migrations/migrate-mongo-config.js",
    "migrate:status": "migrate-mongo status -f migrations/migrate-mongo-config.js",
    "seed:db": "node seed.cjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.77",
    "@sveltejs/kit": "1.0.0-next.511",
    "@tailwindcss/forms": "^0.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/nodemailer": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.5",
    "classnames": "^2.3.1",
    "cssnano": "^5.1.7",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "migrate-mongo": "^9.0.0",
    "phosphor-svelte": "^1.2.1",
    "postcss": "^8.4.13",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.3"
  },
  "type": "module",
  "dependencies": {
    "@trpc/client": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "argon2": "^0.29.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "4.10",
    "nodemailer": "^6.8.0",
    "trpc-sveltekit": "^2.3.6",
    "trpc-transformer": "^2.1.3",
    "zod": "^3.19.1"
  }
}
